   "source": [
    "# Splice the data between passing and failing drugs\n",
    "fig, ax = plt.subplots()\n",
    "ind = np.arange(len(Media_polarity))  \n",
    "width = .75\n",
    "rectBBC = ax.bar(ind[0], Media_polarity[0], width, color='coral')\n",
    "rectCBS = ax.bar(ind[1], Media_polarity[1], width, color='gold')\n",
    "rectCNN = ax.bar(ind[2], Media_polarity[2], width, color='blue')\n",
    "rectFoxNews = ax.bar(ind[3], Media_polarity[3], width, color='red')\n",
    "rectnytimes = ax.bar(ind[4], Media_polarity[4], width, color='green')\n",
    "\n",
    "# Orient widths. Add labels, tick marks, etc. \n",
    "ax.set_ylabel('Tweet Popularity')\n",
    "ax.set_title('Overall Media Sentiment based on Twitter (03/26/2018)')\n",
    "ax.set_xticks(ind)\n",
    "ax.set_xticklabels(('BBC', 'CBS', 'CNN','Fox' ,'NY Times'))\n",
    "ax.set_autoscaley_on(True)\n",
    "ax.grid(True)\n",
    "\n",
    "# Use functions to label the percentages of changes\n",
    "def autolabelFail(rects):\n",
    "    for rect in rects:\n",
    "        height = rect.get_height()\n",
    "        ax.text(rect.get_x() + rect.get_width()/2., .05,\n",
    "                '+%.2f' % float(height),\n",
    "                ha='center', va='bottom', color=\"black\")\n",
    "\n",
    "def autolabelPass(rects):\n",
    "    for rect in rects:\n",
    "        height = rect.get_height()\n",
    "        ax.text(rect.get_x() + rect.get_width()/2., -.05,\n",
    "                '-%.2f ' % float(height),\n",
    "                ha='center', va='bottom', color=\"black\")\n",
    "\n",
    "# Call functions to implement the function calls\n",
    "autolabelFail(rectBBC)\n",
    "autolabelFail(rectCBS)\n",
    "autolabelPass(rectCNN)\n",
    "autolabelPass(rectFoxNews)\n",
    "autolabelPass(rectnytimes)\n",
    "\n",
    "plt.savefig(\"Fig2.png\")\n"
   ]
